{
  "name": "clumsy-math",
  "version": "4.0.0",
  "description": "a math library for the clumsy and curious",
  "main": "./package_source/index.js",
  "types": "./package_source/types/index.js",
  "license": "tacoware",
  "repository": {
    "type": "git",
    "url": "https://github.com/clumsycomputer/clumsy-math"
  },
  "keywords": [
    "math",
    "typescript"
  ],
  "scripts": {
    "buildPackageSource": "tsc --project ./library/tsconfig.json && cp -r ./library/package_source ./package_source && rm -rf ./library/package_source",
    "buildDevelopmentPackage": "yarn buildPackageSource && mkdir ./clumsy-math && cp -r ./package* ./clumsy-math && rm -rf ./node_modules/clumsy-math && cp -r ./clumsy-math ./node_modules && rm -rf ./clumsy-math && rm -rf ./package_source",
    "developWebsite": "NODE_OPTIONS=--openssl-legacy-provider preact watch --src ./website/source --host 127.0.0.1"
  },
  "dependencies": {
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "preact": "^10.14.1",
    "preact-cli": "^3.4.5",
    "preact-router": "^4.1.1",
    "sass": "^1.62.1",
    "sass-loader": "10.4.1"
  }
}
